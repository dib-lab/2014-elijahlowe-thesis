	keywords = {Amino Acid Sequence, Animals, Cell Cycle, Cell Cycle: genetics, Cell Cycle Proteins, Cell Cycle Proteins: genetics, Cell Cycle Proteins: metabolism, Cell Differentiation, Cell Differentiation: genetics, Cell Size, Cell Size: genetics, Ciona intestinalis, Ciona intestinalis: embryology, Ciona intestinalis: genetics, Ciona intestinalis: metabolism, Cytoskeleton, Cytoskeleton: genetics, Cytoskeleton: metabolism, Embryo, Fetal Proteins, Molecular Sequence Data, Nonmammalian, Notochord, Notochord: embryology, Notochord: metabolism, Sequence Alignment, T-Box Domain Proteins, T-Box Domain Proteins: genetics, T-Box Domain Proteins: metabolism},
	pages = {69--80},
	file = {Attachment:/Users/elijahklowe/Library/Application Support/Zotero/Profiles/10oxqg53.default/zotero/storage/XEUXJDZ5/Hotta et al. - 2000 - Characterization of Brachyury-downstream notochord genes in the Ciona intestinalis embryo.pdf:application/pdf}
