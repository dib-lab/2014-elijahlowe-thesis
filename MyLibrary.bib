@article{myers_convergence_2002,
	title = {Convergence and extension in vertebrate gastrulae: cell movements according to or in search of identity?},
	volume = {18},
	issn = {0168-9525},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/12175805},
	abstract = {During vertebrate gastrulation, convergence and extension cell movements both narrow and lengthen the forming embryonic axis. Concurrently, positional information established principally by the ventral-to-dorsal gradient of bone morphogenetic protein activity specifies cell fates within the gastrula. New data, primarily from zebrafish, have identified domains of distinct convergence and extension movements, and have established a role for the noncanonical Wnt signaling pathway in promoting the mediolateral cell polarization that underlies this morphogenetic process. Other observations suggest the intriguing possibility that positional information regulates convergence and extension movements in parallel with cell-fate specification.},
	number = {9},
	journal = {Trends in genetics : {TIG}},
	author = {Myers, Dina C and Sepich, Diane S and Solnica-Krezel, Lilianna},
	month = sep,
	year = {2002},
	keywords = {Animals, Body Patterning, Body Patterning: physiology, Bone Morphogenetic Proteins, Bone Morphogenetic Proteins: physiology, Cell Movement, Cell Movement: physiology, Developmental, Developmental: physiol, Embryo, Gastrula, Gastrula: physiology, Gene Expression Regulation, Nonmammalian, Nonmammalian: physiology, Signal Transduction, zebrafish, Zebrafish: embryology, Zebrafish: physiology},
