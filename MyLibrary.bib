	keywords = {Amino Acid Sequence, Animals, {DNA}, Ribosomal, Evolution, Molecular, Gene Duplication, Homeodomain Proteins, Invertebrates, Molecular Sequence Data, phylogeny, {RNA}, Ribosomal, 18S, Sequence Homology, Amino Acid, Vertebrates},
	pages = {366--373}
