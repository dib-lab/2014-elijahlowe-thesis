	keywords = {Animals, Biological, Body Patterning, Cell Communication, Cell Polarity, Developmental, Drosophila, Drosophila: genetics, Drosophila: metabolism, Epithelium, Epithelium: metabolism, Frizzled Receptors, Frizzled Receptors: metabolism, Gene Expression Regulation, Models, Morphogenesis},
