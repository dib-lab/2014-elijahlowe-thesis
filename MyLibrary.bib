@article{bronner-fraser_neural_1994,
	title = {Neural crest cell formation and migration in the developing embryo.},
	volume = {8},
	issn = {0892-6638},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/8050668},
	abstract = {Neural crest cells arise from the neural tube shortly after its closure and migrate extensively through prescribed regions of the embryos, where they differentiate into most of the peripheral nervous system as well as the facial skeleton and pigment cells. Along the embryonic axis, several distinct neural crest populations differ both in their migratory pathways and range of derivatives. Whereas those cells arising from the midbrain migrate as a uniform sheet of cells, neural crest cells emerging from the hindbrain and trunk regions migrate in a segmented manner. For example, trunk neural crest cells move preferentially through the rostral, but not caudal, half of each somite. Interactions with tissues encountered during migration strongly influence this segmental migratory pattern. For example, the mesodermal somites dictate the segmental migration of trunk neural crest cells and the otic placode appears to attract hindbrain neural crest cells. Although little is known about the molecular basis underlying migration, patterns of gene expression in the hindbrain are thought to contribute to the segmental arrangement of neural crest cells. Furthermore, neural crest cells possess integrin receptors that may be important for interacting with extracellular matrix molecules in their surroundings.},
	number = {10},
	journal = {The {FASEB} journal : official publication of the Federation of American Societies for Experimental Biology},
	author = {Bronner-Fraser, M},
	month = jul,
	year = {1994},
	keywords = {Cell Movement, Cell Polarity, Cell Surface, Cell Surface: metabolism, Embryonic and Fetal Development, Embryonic and Fetal Development: genetics, Gene Expression, Neural Crest, Neural Crest: cytology, Neural Crest: embryology, Receptors, Rhombencephalon, Rhombencephalon: embryology, Rhombencephalon: metabolism, Skull},
	pages = {699--706},
	file = {Attachment:/Users/elijahklowe/Library/Application Support/Zotero/Profiles/10oxqg53.default/zotero/storage/ZN32MV8J/Bronner-Fraser - 1994 - Neural crest cell formation and migration in the developing embryo.pdf:application/pdf}
}

@article{jones_planar_2007,
	title = {Planar cell polarity signaling in vertebrates.},
	volume = {29},
	issn = {0265-9247},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/17226800},
	doi = {10.1002/bies.20526},
	abstract = {Planar cell polarity ({PCP}) refers to the polarization of a field of cells within the plane of a cell sheet. This form of polarization is required for diverse cellular processes in vertebrates, including convergent extension ({CE}), the establishment of {PCP} in epithelial tissues and ciliogenesis. Perhaps the most distinct example of vertebrate {PCP} is the uniform orientation of stereociliary bundles at the apices of sensory hair cells in the mammalian auditory sensory organ. The establishment of {PCP} in the mammalian cochlea occurs concurrently with {CE} in this ciliated epithelium, therefore linking three cellular processes regulated by the vertebrate {PCP} pathway in the same tissue and emerging as a model system for dissecting {PCP} signaling. This review summarizes the morphogenesis of this model system to assist the interpretation of the emerging data and proposes molecular mechanisms underlying {PCP} signaling in vertebrates.},
	number = {2},
	journal = {{BioEssays} : news and reviews in molecular, cellular and developmental biology},
	author = {Jones, Chonnettia and Chen, Ping},
	month = feb,
	year = {2007},
	keywords = {Animals, Biological, Cell Polarity, Cell Polarity: genetics, Epithelial Cells, Epithelial Cells: physiology, Humans, Models, Morphogenesis, Organ of Corti, Organ of Corti: cytology, Organ of Corti: growth \& development, Organ of Corti: metabolism},
	pages = {120--32},
	file = {Attachment:/Users/elijahklowe/Library/Application Support/Zotero/Profiles/10oxqg53.default/zotero/storage/RWISUW82/Jones, Chen - 2007 - Planar cell polarity signaling in vertebrates.pdf:application/pdf}
