	month = dec,
	year = {2000},
	keywords = {Ascidian, Atrial siphon, Ciona intestinalis, Development, Distal-less, Homeobox-containing genes, Myoplasm},
	pages = {173--176},
	file = {ScienceDirect Full Text PDF:/Users/elijahklowe/Library/Application Support/Zotero/Profiles/10oxqg53.default/zotero/storage/6A8BU4EP/Caracciolo et al. - 2000 - Identification and developmental expression of thr.pdf:application/pdf;ScienceDirect Snapshot:/Users/elijahklowe/Library/Application Support/Zotero/Profiles/10oxqg53.default/zotero/storage/SJXF5IRD/S0925477300004743.html:text/html}
